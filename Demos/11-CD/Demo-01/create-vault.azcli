rnd=$RANDOM
grp=az400-m11
loc=westeurope
vault=foodvault-$rnd

az group create -n $grp -l $loc

az keyvault create -l $loc -n $vault -g $grp --sku standard 

az keyvault key list --vault-name $vault

az keyvault secret set --vault-name $vault --name "SQLAdmin" --value "azsqladmin"

az keyvault secret set --vault-name $vault --name "SQLAdminPWD" --value "P4azsql@dmin123"