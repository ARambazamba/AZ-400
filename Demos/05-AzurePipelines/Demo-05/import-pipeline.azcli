# Add the extension
az extension add --name azure-devops

# List help
az devops -help

# Log in to your tenant
az devops configure --defaults organization=https://dev.azure.com/integrationstraining

# List projects
az devops project list -o table

# Create a pipeline
az pipelines create --name "CLIPipeline" --project AZ-400-T01 --repository CoreBuild --repository-type tfsgit --branch master

